{
  "name": "KCAMathsHub",
  "version": "0.0.0",
  "description": "A community-based platform for providing peer-assistance with Math school work and questions; currently available internally at KCA.",
  "main": "index.js",
  "repository": "https://github.com/timthedev07/KCAMathsHub",
  "author": "Tim <timpersonal07@gmail.com>",
  "contributors": [
    "Mykola <mykola.starovoit@gmail.com>",
    "Elena <hehewohaowanmei@gmail.com>"
  ],
  "license": "LGPL-3.0-or-later",
  "private": true,
  "prisma": {
    "seed": "yarn populate"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:migrate": "dotenv -e .env.local -- prisma migrate dev --name",
    "dev:studio": "dotenv -e .env.local -- prisma studio",
    "dev:db-push": "dotenv -e .env.local -- prisma db push",
    "prod:studio": "dotenv -e .env.prod -- prisma studio",
    "prod:migrate": "dotenv -e .env.prod -- prisma migrate deploy",
    "populate:categories": "dotenv -e .env.local -- ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/scripts/categories.ts",
    "populate:roles": "dotenv -e .env.local -- ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/scripts/prePopulateRoles.ts",
    "populate": "yarn populate:roles && yarn populate:categories",
    "fake:questions": "dotenv -e .env.local -- ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/scripts/dev/fakeq.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.474.0",
    "@aws-sdk/client-s3": "^3.474.0",
    "@aws-sdk/cloudfront-signer": "^3.468.0",
    "@aws-sdk/lib-storage": "^3.474.0",
    "@headlessui/react": "^1.7.17",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mdxeditor/editor": "1.13.5",
    "@next/mdx": "^14.0.4",
    "@prisma/client": "^5.7.0",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@types/mdx": "^2.0.10",
    "autoprefixer": "10.4.16",
    "eslint-config-next": "14.0.4",
    "fast-deep-equal": "^3.1.3",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-connect": "^1.0.0",
    "next-mdx-remote": "^4.4.1",
    "next-nprogress-bar": "^2.1.2",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-auth-provider-buttons": "^0.0.4",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.5.3",
    "remark-gfm": "3.0.1",
    "sharp": "0.32.6",
    "superjson": "^2.2.1",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@types/node": "^20.10.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-mdx": "^3.1.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gen-env-types": "^1.3.4",
    "prisma": "^5.7.0",
    "tailwindcss": "3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
