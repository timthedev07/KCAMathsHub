{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "eslint.options": {
    "extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
  },
  "eslint.validate": [
    "markdown",
    "mdx",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "grammarly.selectors": [
    {
      "language": "mdx",
      "scheme": "file",
      "pattern": "app/src/app/docs/**/*.mdxj"
    }
  ]
}
